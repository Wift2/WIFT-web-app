{
  "name": "wiftai-webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "lint:check": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "fix:line-endings": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" && eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive:true, force:true}); require('fs').rmSync('node_modules/.vite', {recursive:true, force:true})\"",
    "clean:install": "npm run clean && npm install",
    "setup": "npm install && npm run type-check",
    "setup:dev": "npm run setup && npm run dev",
    "amplify:sandbox": "npx ampx sandbox",
    "amplify:generate": "npx ampx generate",
    "amplify:deploy": "npx ampx deploy",
    "postinstall": "npm run type-check"
  },
  "dependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@aws-amplify/ui-react": "^6.11.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/system": "^7.2.0",
    "@mui/x-data-grid": "^8.8.0",
    "@mui/x-date-pickers": "^8.9.0",
    "@toolpad/core": "^0.16.0",
    "@vitejs/plugin-react": "^4.6.0",
    "aws-amplify": "^6.15.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.6",
    "leaflet": "^1.9.4",
    "leaflet-control-geocoder": "^3.2.0",
    "leaflet-measure": "^3.1.0",
    "leaflet.locatecontrol": "^0.84.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unicorn": "^57.0.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
